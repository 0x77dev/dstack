{
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "explorer",
  "extends": "@nrwl/workspace/presets/core.json",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true,
        "linter": "eslint",
        "style": "@emotion/styled"
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "linter": "eslint",
        "style": "@emotion/styled"
      }
    }
  },
  "npmScope": "dstack-js",
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "accessToken": "MjBiYTU3ODktZDVlZC00MmNkLWI2YmQtNmNlNDIwOWY3ZDU1fHJlYWQtd3JpdGU=",
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "serve"
        ]
      },
      "runner": "@nrwl/nx-cloud"
    }
  }
}
