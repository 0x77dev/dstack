{
  "implicitDependencies": [
    "lib",
    "ipfs"
  ],
  "projectType": "application",
  "root": "packages/explorer",
  "sourceRoot": "packages/explorer/src",
  "tags": [],
  "targets": {
    "build": {
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "build"
        }
      ],
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "yarn build"
        ],
        "cwd": "packages/explorer"
      },
      "outputs": [
        "packages/explorer/dist",
        ".parcel-cache"
      ]
    },
    "serve": {
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "build"
        }
      ],
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "yarn serve"
        ],
        "cwd": "packages/explorer"
      },
      "outputs": [
        ".parcel-cache"
      ]
    }
  }
}
