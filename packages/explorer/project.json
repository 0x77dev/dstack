{
  "projectType": "application",
  "root": "packages/explorer",
  "serve": {
    "executor": "@nrwl/workspace:run-commands",
    "options": {
      "commands": [
        "nx build lib",
        "nx build ipfs",
        "cd packages/explorer && yarn serve"
      ],
      "cwd": "/"
    }
  },
  "sourceRoot": "packages/explorer/src",
  "tags": [],
  "targets": {
    "executor": "@nrwl/workspace:run-commands",
    "options": {
      "commands": [
        "nx build lib",
        "nx build ipfs",
        "yarn build"
      ],
      "cwd": "/"
    },
    "outputs": [
      "packages/explorer/dist"
    ]
  }
}
